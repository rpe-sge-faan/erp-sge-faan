<Window x:Class="SGE_erp.Administracion.EmpleadosEditar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SGE_erp.Administracion"
        mc:Ignorable="d"
        Title="EmpleadosEditar" Height="450" Width="800">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="380" VerticalAlignment="Top" Width="770" Margin="10,0,0,0">
            <TabItem Header="General">
                <Grid Background="#FFE5E5E5">
                    <local:EmpleadosGeneral HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch"/>
                </Grid>
            </TabItem>
            <TabItem Header="Observaciones">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,-2"/>
            </TabItem>
        </TabControl>
        <Button Content="_Aceptar" HorizontalAlignment="Left" Margin="538,385,0,0" VerticalAlignment="Top" Width="90"/>
        <Button Content="_Cancelar" HorizontalAlignment="Left" Height="20" Margin="673,385,0,0" VerticalAlignment="Top" Width="90" RenderTransformOrigin="0.488,-0.727"/>

    </Grid>
</Window>
